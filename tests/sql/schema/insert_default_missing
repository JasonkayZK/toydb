CREATE TABLE defaults (
  id INTEGER PRIMARY KEY,
  required BOOLEAN NOT NULL,
  "null" BOOLEAN DEFAULT NULL,
  "boolean" BOOLEAN DEFAULT TRUE,
  "float" FLOAT DEFAULT 3.14,
  "integer" INTEGER DEFAULT 7,
  "string" STRING DEFAULT foo
)
